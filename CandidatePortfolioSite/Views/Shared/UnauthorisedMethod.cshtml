@{
    ViewData["Title"] = "UploadSuccess";
    Layout = null;
}
<style>

    #mySidenav a {
        position: absolute;
        right: -60px;
        transition: 0.3s;
        padding: 15px;
        width: 100px;
        text-decoration: none;
        font-size: 20px;
        color: #4682B4;
        border-radius: 0 5px 5px 0;
        border-color: gold;
    }

        #mySidenav a:hover {
            right: 0;
        }

    #signout {
        top: 20px;
        border-color: #87CEEB;
        background-color: #F0F8FF;
    }

    #login {
        top: 80px;
        border-color: #87CEEB;
        background-color: #F0F8FF;
    }

    #candidates {
        top: 165px;
        border-color: #87CEEB;
        background-color: #F0F8FF;
    }



    body {
        background-color: #ADD8E6;
    }

    .container {
        padding: 15px;
        background-color: #F0F8FF;
        border-radius: 15px;
        margin: auto;
        width: 80%;
    }

    .titleContainer {
        padding: 15px;
        background-color: #F0F8FF;
        border-radius: 15px;
        margin: auto;
        width: 80%;
    }



    .MapContainer {
        padding: 50px;
        background-color: #F0F8FF;
        border-radius: 15px;
        margin: auto;
        width: 80%;
        height: 600px;
        display: none;
        margin-left: auto;
        margin-right: auto;
    }


    .innerContainer {
        padding: 15px;
        background-color: #F0F8FF;
        margin: auto;
        width: 30%;
    }

    .iconImage {
        padding: 5px;
        background-color: #F0F8FF;
        margin: auto;
        width: 80%;
    }

    .center {
        display: block;
        margin-left: auto;
        margin-right: auto;
        width: 50%;
    }

    .MapCenter {
        display: block;
        margin-left: auto;
        margin-right: auto;
        width: 98%;
        height: 100%;
    }

    .insert {
        display: block;
        margin-left: auto;
        margin-right: auto;
        width: 90%;
    }

    input[type=text] {
        border: 2px solid #4682B4;
        border-radius: 4px;
    }

    input[type=password] {
        border: 2px solid #4682B4;
        border-radius: 4px;
    }


    table {
        border-collapse: collapse;
        width: 100%;
    }

    th, td {
        padding: 8px;
        text-align: left;
        border-bottom: 1px solid #ddd;
    }

    tr:hover {
        background-color: #ADD8E6;
    }

    .switch {
        position: relative;
        display: inline-block;
        width: 60px;
        height: 34px;
    }

        .switch input {
            opacity: 0;
            width: 0;
            height: 0;
        }

    .slider {
        position: absolute;
        cursor: pointer;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background-color: #ccc;
        -webkit-transition: .4s;
        transition: .4s;
    }

        .slider:before {
            position: absolute;
            content: "";
            height: 26px;
            width: 26px;
            left: 4px;
            bottom: 4px;
            background-color: white;
            -webkit-transition: .4s;
            transition: .4s;
        }

    input:checked + .slider {
        background-color: #2196F3;
    }

    input:focus + .slider {
        box-shadow: 0 0 1px #2196F3;
    }

    input:checked + .slider:before {
        -webkit-transform: translateX(26px);
        -ms-transform: translateX(26px);
        transform: translateX(26px);
    }

    /* Rounded sliders */
    .slider.round {
        border-radius: 34px;
    }

        .slider.round:before {
            border-radius: 50%;
        }


    .listPhrase {
        display: block;
    }

    .mapPhrase {
        display: none;
    }

    #myInput {
        background-image: url('../../Files/search.png');
        background-size: 20px 20px;
        background-position: 10px 10px;
        background-repeat: no-repeat;
        width: 100%;
        font-size: 16px;
        padding: 12px 20px 12px 40px;
        border: 1px solid #ddd;
        margin-bottom: 12px;
    }
</style>
